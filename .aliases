alias npublish="npm version patch && npm publish && git push origin master --tags"
alias tree="tree -CFa -I 'rhel.*.*.package|.git|node_modules' --dirsfirst"
alias nodedev="NODE_ENV=development node $1"
alias nodetest="NODE_ENV=test node $1"
alias nodeprod="NODE_ENV=production node $1"
alias ntest="NODE_ENV=test npm test"
alias npmcov="npm run cover && npm run view-cover"
alias gpublish="npm version patch && git push origin master --tags"
alias npmrun='npm -s run'
